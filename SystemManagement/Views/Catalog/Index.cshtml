@model SystemManagement.Models.AssetIndexModel  <h1>Catalog Index</h1>  <div id="assets">     <div id="assetsTable">         <table class="table table condensed" id="DataIndexTable">              <thead>                 <tr>                     <th>Image</th>                     <th>Description</th>
                </tr>             </thead>             <tbody>                 @foreach (var asset in Model.Assets)
{                 <tr>                     <td class="">                         <a asp-controller="Catalog" asp-action="Detail" asp-route-id="@asset.Id">                             <img src="@asset.ImageUrl" class="Image-cell" />                         </a>                     </td>                     <td class="">@asset.Description</td>                  </tr>
}             </tbody>         </table>     </div> </div>
